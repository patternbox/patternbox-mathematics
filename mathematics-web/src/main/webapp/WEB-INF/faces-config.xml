<?xml version="1.0"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
              version="2.0">
    <application>
        <locale-config>
            <default-locale>en</default-locale>
            <supported-locale>de</supported-locale>
        </locale-config>
        <resource-bundle>
            <base-name>com.patternbox.mathematics.web.messages.messages</base-name>
            <var>msgs</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>com.patternbox.mathematics.web.messages.fibonacci</base-name>
            <var>fibo</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>com.patternbox.mathematics.web.messages.eratosthenes</base-name>
            <var>eratos</var>
        </resource-bundle>
    </application>
</faces-config>